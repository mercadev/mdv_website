<table id='contact_table' class="table table-striped table-hover table-bordered">
	<thead class="thead-dark">
		<tr>
			<th scope="col">Id</th>
			<th scope="col">Contact</th>
			<th scope="col">Statut</th>
			<th scope="col">Nombre de mails envoy√©s</th>
		</tr>
	</thead>
	<tbody>
		<% @logs.each do |l| %>
		<tr>
			<th scope="row"><%= l.id %></th>
			<td><%= l.first_name %></td>
			<td><%= l.last_name %></td>
			<td><%= l.company %></td>
			<td><%= l.activity %></td>
			<td><%= l.profession %></td>
			<td><%= l.email %></td>
			<td><%= l.mobile_phone %></td>
			<td><%= l.fixed_phone %></td>
			<td><%= l.address %></td>
			<td><%= l.website %></td>
			<td><%= l.source %></td>
			<td><%= check_box_tag 'contact_ids[]', l.id, false, class: 'contact-checkbox' %></td>
		</tr>
		<% end %>
	</tbody>
</table>